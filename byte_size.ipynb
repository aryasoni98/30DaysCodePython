   "source": [
    "# Returns the length of a string in bytes.\n",
    "\n",
    "    Use s.encode('utf-8') to encode the given string and return its length.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def byte_size(s):\n",
    "  return len(s.encode('utf-8'))"
   ]
  },
